{
    "extends": [
        "google",
        "angular"
    ],
    "parser": "babel-eslint",
    "globals": {
        "SwaggerEditor": false
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "no-invalid-this": "off",
        "max-len": "off",
        "require-jsdoc": "off",
        "no-var": "off",
        "no-extend-native": "off",
        "no-tabs": "off"
    }
}